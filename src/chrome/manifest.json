{
    "name": "webtimer",
    "version": "0.1.0",
    "author": "jh2946",
    "manifest_version": 3,
    "description": "track how much time you spend on websites",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "storage",
        "unlimitedStorage",
        "offscreen"
    ],
    "action": {
        "default_title": "webtimer",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icon.png"
    }
}